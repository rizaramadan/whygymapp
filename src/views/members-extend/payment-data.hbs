<table>
  <thead>
    <tr>
      <th>Member Email</th>
      <th>Order ID</th>
      <th>Extra Time</th>
      <th>Amount</th>
      <th>Created At</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
      <tr>
        <td>{{memberEmail}}</td>
        <td>{{orderId}}</td>
        <td>{{extras}}</td>
        <td>...</td>
        <td>{{formatDate createdAt}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

<style>
  table {
    width: 100%;
    border-collapse: collapse;
    margin: 20px 0;
    font-family: Arial, sans-serif;
  }

  th, td {
    padding: 12px;
    text-align: left;
    border-bottom: 1px solid #ddd;
  }

  th {
    background-color: #f5f5f5;
    font-weight: bold;
  }

  tr:hover {
    background-color: #f9f9f9;
  }

  td {
    color: #333;
  }
</style> 