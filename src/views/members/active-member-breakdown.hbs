<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Gender</th>
      <th>Promo Type</th>
      <th>Group Type</th>
      <th>Duration</th>
      <th>Start Date</th>
      <th>ID</th>
      <th>Extra Time</th>
      <th>Week Setting</th>
      <th>Full Name</th>
      <th>Tel</th>
    </tr>
  </thead>
  <tbody>
    {{#each breakdown}}
      <tr>
        <td>{{email}}</td>
        <td>{{gender}}</td>
        <td>{{promoType}}</td>
        <td>{{groupType}}</td>
        <td>{{duration}}</td>
        <td>{{extractDate startDate}}</td>
        <td>{{id}}</td>
        <td>{{totalExtra}}</td>
        <td>{{weekSetting}}</td>
        <td>{{fullname}}</td>
        <td>{{tel}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
